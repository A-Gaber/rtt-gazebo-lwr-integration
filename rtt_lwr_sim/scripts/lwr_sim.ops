// Launching LWR SIM
loadComponent("gazebo","CORBA")

import("rtt_ros")
import("rtt_rosnode")
ros.import("rtt_lwr_sim")

loadComponent("lwr_sim","lwr::LWRSim")

setActivity("lwr_sim",0.001,LowestPriority,ORO_SCHED_OTHER)

connectPeers("gazebo","lwr_sim")

lwr_sim.configure()
lwr_sim.start()


runScript(ros.find("lwr_state_publisher")+"/rtt_lwr_state_publisher.ops")

